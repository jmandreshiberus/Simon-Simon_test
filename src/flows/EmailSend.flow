    <decisions>
        <name>Lead_OR_Contact</name>
        <label>Lead OR Contact</label>
        <locationX>628</locationX>
        <locationY>160</locationY>
        <defaultConnectorLabel>[Resultado predeterminado]</defaultConnectorLabel>
        <rules>
            <name>isLead</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Var_LeadId</leftValueReference>
                <operator>StartsWith</operator>
                <rightValue>
                    <stringValue>00Q</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Lead_Details</targetReference>
            </connector>
            <label>isLead</label>
        </rules>
        <rules>
            <name>isContact</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Var_LeadId</leftValueReference>
                <operator>StartsWith</operator>
                <rightValue>
                    <stringValue>003</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Contact_Details</targetReference>
            </connector>
            <label>isContact</label>
        </rules>
    </decisions>
    <decisions>
        <name>Mandar_correo_de_rechazo_o_consentimiento</name>
        <label>Mandar correo de rechazo o consentimiento</label>
        <locationX>625</locationX>
        <locationY>423</locationY>
        <defaultConnectorLabel>Defecto</defaultConnectorLabel>
        <rules>
            <name>Consentimiento</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Var_Consentimiento</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Var_Rechazo</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Consentimiento_mail</targetReference>
            </connector>
            <label>Consentimiento</label>
        </rules>
        <rules>
            <name>Rechazo_dcs</name>
            <conditionLogic>(1 AND 2) OR (3 AND 4)</conditionLogic>
            <conditions>
                <leftValueReference>Var_Consentimiento</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Var_Rechazo</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Var_Consentimiento</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Var_Rechazo</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Rechazo_mail</targetReference>
            </connector>
            <label>Rechazo</label>
        </rules>
    </decisions>
